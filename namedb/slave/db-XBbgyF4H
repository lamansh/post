$ORIGIN .
$TTL 3600	; 1 hour
nchru.ipsec		IN SOA	ns.nchadvisors.ru. bubba.nchadvisors.ru. (
				2013051301 ; serial
				3600       ; refresh (1 hour)
				900        ; retry (15 minutes)
				3600000    ; expire (5 weeks 6 days 16 hours)
				3600       ; minimum (1 hour)
				)
			NS	ns.nchru.ipsec.
			HINFO	"i586" "FreeBSD-4.10"
			MX	10 mx1.nchru.ipsec.
			MX	20 mx2.nchru.ipsec.
$ORIGIN nchru.ipsec.
